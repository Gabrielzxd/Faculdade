tipos 
    PNo = ponteiro para TNo
    TNo = registro
            info: inteiro
            prox: PNo
        fim registro

função ListaOrdemAscendente(l: PNo): lógico
início
    se l != Nulo e l^.prox = Nulo então
        retorne verdadeiro
    senão
        se l = Nulo então
            retorne falso
        senão
            se l^.info < l^.prox^.info então
                retorne ListaOrdemAscendente(l^.prox)
            senão
                retorne falso
            fim se
        fim se
    fim se
fim função  