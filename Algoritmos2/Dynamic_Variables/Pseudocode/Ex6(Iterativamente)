tipos
    PNo = ponteiro para TNo
    TNo = registro
            info: string
            prox: PNo
        fim registro
var
    função StringNaPosiçãoDada(p: inteiro; l: PNo): string
    var
        i: inteiro
    início
        i <- 1
        enquanto l != Nulo faça
            se i = p então
                retorne l^.info
            fim se
            i <- i + 1
            l <- l^prox
        fim enquanto
        retorne ""
    fim função