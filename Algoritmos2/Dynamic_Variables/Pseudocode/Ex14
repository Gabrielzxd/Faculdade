tipos
    PNo = ponteiro para TNo
    TNo = registro
            info: string
            prox: PNo
        fim registro

função InsereNoInício(ref l: PNo; x: string): lógico
var
    novo: Pno
início
    novo <- aloca(TNo)
    se novo = Nulo então
        retorne falso
    fim se
    novo^.info <- x
    novo^.prox <- l
    l <- novo
    retorne verdadeiro
fim função