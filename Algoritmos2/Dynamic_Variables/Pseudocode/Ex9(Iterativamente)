tipos
    PNo = ponteiro para TNo
    TNo = registro
            info: TInfo
            prox: PNo
        fim registro
var
    função VerificaSeElementosAparecemJuntos(x, y: real; l: PNo): lógico
    var
        achou: lógico
        ant: PNo
    início
        achou <- falso
        enquanto l != Nulo faça
            ant <- l
            l <- l^prox
            achou <- ant^info.valor = x e l^info.valor = y
            se achou então
                retorne achou
            fim se
        fim enquanto
        retorne achou
    fim função