tipos
    PNo = ponteiro para TNo
    TNo = registro
            info: real
            prox: PNo
        fim registro
var
    função TamanhoListaEncadeada(l: PNo): inteiro
    var
        tam: inteiro
        atual: PNo
    início
        se l = Nulo então
            retorne 0
        fim se
        tam <- 1
        enquanto l != Nulo faça
            tam <- tam + 1
            l <- l^.prox
        fim enquanto
        retorne tam
    fim função