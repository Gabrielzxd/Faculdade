tipos
    PNo = ponteiro para TNo
    TNo = registro
            info: string
            prox: PNo
        fim registro

função RemoveUltimaPosição(ref l: PNo): lógico
var
    lixo: PNo
início
    se l = Nulo então
        retorne falso
    fim se
    enquanto l^.prox != Nulo faça
        l <- l^.prox
    fim enquanto
    lixo <- l
    l <- l^.prox
    desaloca(lixo)
    retorne verdadeiro
fim função