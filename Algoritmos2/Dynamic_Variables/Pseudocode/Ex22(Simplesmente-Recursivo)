tipos
    PNo = ponteiro para TNo
    TNo = registro
            info: string
            prox: PNo
        fim registro

função RemoveDadoElemento(ref l: PNo; x: string): lógico
var
    lixo: PNo
início
    se l = Nulo então
        retorne falso
    fim se
    se l^.info = x então
        lixo <- l
        l <- l^.prox
        desaloca(lixo)
        retorne verdadeiro
    fim se
fim função