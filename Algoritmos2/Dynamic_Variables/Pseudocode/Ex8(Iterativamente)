tipos
    PNo = ponteiro para TNo
    TNo = registro
            info: string
            prox: PNo
        fim registro
var
    função MaiorValor(l: PNo): string
    var
        maior: string
    início
        maior <- ""
        enquanto l != Nulo faça
            se l^.info > maior então
                maior <- l^.info
            fim se
            l <- l^.prox
        fim enquanto
        retorne maior
    fim função
