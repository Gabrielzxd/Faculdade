função OctalStringParaDecimal(octal: string): inteiro
var
    função VerificaOctal(i: inteiro): lógico
    início
        se i = 1 então
            retorne octal[i] = '-' ou '0' <= octal[i] e octal[i] <= '7'
        fim 
        retorne VerificaOctal(i - 1) e '0' <= octal[i] e octal[i] <= '7'
    fim função
    função Transformar(i: inteiro): inteiro
    início
        se i = 1 então
            se octal[i] = '-' então
                retorne 0
            senão
                retorne asc(octal[i]) - asc('0')
            fim se
        fim se
        retorne Transformar(i-1)*8 + asc(octal[i]) - asc('0')
    fim função
início
    se tamanho(s) = 0 ou não VerificaOctal(octal) então
        retorne -999999
    fim se
    se octal[1] = '-' então
        retorne - Transformar(tamanho(octal))
    senão
        retorne Transformar(tamanho(octal))
    fim se
fim função